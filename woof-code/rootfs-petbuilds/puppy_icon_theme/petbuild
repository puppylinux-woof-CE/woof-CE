download() {
    [ -f puppy_icon_theme-master.zip ] || wget -t 1 -T 15 -O puppy_icon_theme-master.zip https://github.com/puppylinux-woof-CE/puppy_icon_theme/archive/refs/heads/master.zip
}

build() {
    unzip -q puppy_icon_theme-master.zip
    cd puppy_icon_theme-master
    export THEME="Puppy Standard"
    make && make install
}
