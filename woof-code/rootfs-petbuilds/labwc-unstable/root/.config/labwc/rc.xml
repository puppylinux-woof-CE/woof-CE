<?xml version="1.0"?>

<labwc_config>
  <theme>
    <name></name>
    <cornerRadius>5</cornerRadius>
  </theme>

  <keyboard>
    <default />

    <keybind key="A-F1">
      <action name="ShowMenu"><menu>root-menu</menu></action>
    </keybind>

    <keybind key="A-F2">
      <action name="Execute"><command>defaultrun</command></action>
    </keybind>

    <keybind key="W-l">
      <action name="Execute"><command>puplock</command></action>
    </keybind>

    <keybind key="W-r">
      <action name="Execute"><command>defaultrun</command></action>
    </keybind>

    <keybind key="C-A-t">
      <action name="Execute"><command>defaultterminal</command></action>
    </keybind>

    <keybind key="Print">
      <action name="Execute"><command>grim</command></action>
    </keybind>

    <keybind key="C-A-Delete">
      <action name="Execute"><command>defaultprocessmanager</command></action>
    </keybind>

    <keybind key="C-A-Backspace">
      <action name="Execute"><command>wmexit</command></action>
    </keybind>

  </keyboard>

  <libinput>
    <device category="default">
      <naturalScroll>no</naturalScroll>
      <leftHanded></leftHanded>
      <pointerSpeed></pointerSpeed>
      <accelProfile></accelProfile>
      <tap>yes</tap>
      <tapButtonMap></tapButtonMap>
      <middleEmulation></middleEmulation>
      <disableWhileTyping></disableWhileTyping>
    </device>
  </libinput>

</labwc_config>
