
mkdir -p usr/share/applications

echo '[Desktop Entry]
Encoding=UTF-8
Name=gFnRename batch file renamer
Icon=/usr/share/pixmaps/puppy/rename.svg
Comment=gFnRename batch file renamer
Exec=gfnrename
Terminal=false
Type=Application
Categories=X-FilesystemUtility
GenericName=gFnRename
' > usr/share/applications/gFnRename.desktop

