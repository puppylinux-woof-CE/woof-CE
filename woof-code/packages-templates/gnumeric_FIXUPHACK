#!/bin/sh

mkdir -p usr/share/applications

cat > usr/share/applications/gnumeric.desktop <<EOF
[Desktop Entry]
Encoding=UTF-8
Name=Gnumeric spreadsheet editor
Icon=/usr/share/icons/hicolor/48x48/apps/gnumeric.png
Comment=Gnumeric spreadsheet editor
Exec=gnumeric
Terminal=false
Type=Application
Categories=X-Calculate
GenericName=Gnumeric spreadsheet editor
EOF

### END ###