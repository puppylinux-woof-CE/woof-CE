#! /bin/bash

export MAIN_DIALOG='
  <vbox>
    <hbox>
      <text><label>Label1</label></text>
      <entry></entry>
    </hbox>
    <hbox>
      <text><label>Label2</label></text>
      <entry></entry>
    </hbox>
    <hbox>
      <button cancel></button>
      <button help></button>
    </hbox>
  </vbox>
'

gtkdialog3 --program=MAIN_DIALOG


