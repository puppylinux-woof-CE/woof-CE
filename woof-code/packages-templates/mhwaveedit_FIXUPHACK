
rm -rf usr/share/pixmaps usr/share/pixmaps
rm -f usr/share/applications/mhwaveedit.desktop

mkdir -p usr/share/applications

echo '[Desktop Entry]
Encoding=UTF-8
Name=mhWaveEdit audio recorder/editor
Icon=/usr/share/pixmaps/puppy/edit_sound.svg
Comment=Audio recorder/editor
Exec=mhwaveedit
Terminal=false
Type=Application
Categories=AudioVideo
GenericName=mhWaveEdit
' > usr/share/applications/mhwaveedit-sound-recorder-editor.desktop

