# fbpanel <profile> config file
# see http://fbpanel.sf.net/docs.html for complete configuration guide


Global {
    edge = bottom
    allign = left
    margin = 0
    widthtype = percent
    width = 100
    height = 26
    transparent = false
    tintcolor = #ffffff
    alpha = 37
    setdocktype = true
    setpartialstrut = true
    autohide = false
    heightWhenHidden = 2
}

Plugin {
    type = space
    config {
        size = 2
    }
}


