#!/bin/bash
#generates analog clock as a svg-file to stdout
#
#usage:
# svg_analogclock WIDTH GMT
#
# WIDTH are are defined number of pixels
# GMT is defined by integer
#
#Sigmund Berglund, aug 2014
#GPL

SIZE="$1"
[ ! "$SIZE" ] && SIZE=200

GMT="$2"
[ ! "$GMT" ] && GMT=0

#---
SEC="`date +%S`"
SEC_ANGLE="`echo "$SEC * 6" | bc`"
MIN="`date +%M`"
MIN_ANGLE="`echo "$MIN * 6" | bc`"
#calculate worldclock
MYTIME="`echo "$MYGMT" | cut -c4- | sed -e 's,-,x,' -e 's,+,-,' -e 's,x,+,'`" #swap it for calculation
NEWTIME="`echo "$1" | cut -d' ' -f1 | cut -c4-`"
GMT_DIFF="$(($NEWTIME+$MYTIME))"
HOUR_ANGLE=$((($(date +%l)+$GMT_DIFF)*30+($MIN_ANGLE/12)))
