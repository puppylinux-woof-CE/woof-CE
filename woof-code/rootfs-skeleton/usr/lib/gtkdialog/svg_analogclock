#!/bin/bash
#generates analog clock (background) as svg-file to stdout
#
#usage:
# svg_analogclock WIDTH
#
# WIDTH are are defined number of pixels
#
#Sigmund Berglund, 2014
#GPL

SIZE="$1"; [ ! "$SIZE" ] && SIZE=220
GMT="$2"; [ ! "$GMT" ] && GMT='GMT-0'

SEC="`date +%S`"
SEC_ANGLE="`echo "$SEC * 6" | bc`"
MIN="`date +%M`"
MIN_ANGLE="`echo "$MIN * 6" | bc`"
#calculate worldclock
MYGMT="`readlink /etc/localtime | cut -d/ -f6 | sed -e 's,^GMT-,GMTx,' -e 's,^GMT+,GMT-,' -e 's,^GMTx,GMT+,'`"
MYTIME="`echo "$MYGMT" | cut -c4- | sed -e 's,-,x,' -e 's,+,-,' -e 's,x,+,'`" #swap it for calculation
NEWTIME="`echo "$GMT" | cut -d' ' -f1 | cut -c4-`"
GMT_DIFF="$(($NEWTIME+$MYTIME))"
HOUR_ANGLE=$((($(date +%l)+$GMT_DIFF)*30+($MIN_ANGLE/12)))

COLOR1="$3"; [ ! "$COLOR1" ] && COLOR1='#E6F1DD'
COLOR2="$4"
[ "$COLOR2" ] && COLOR='url(#RG1)' || COLOR="$COLOR1"

echo '<?xml version="1.0" encoding="utf-8"?>
<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="'$SIZE'" height="'$SIZE'" viewBox="0 0 512 512">
	<title>SVG analog clock</title>

