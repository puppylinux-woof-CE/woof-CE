#!/bin/sh 

if which file-roller &>/dev/null ; then
	exec file-roller "$@"

elif which engrampa &>/dev/null ; then
	exec engrampa "$@"

elif which pupzip &>/dev/null ; then
	exec pupzip "$@"

elif which xarchiver &>/dev/null ; then
	exec xarchiver "$@"
	
elif which xarchive &>/dev/null ; then
	exec xarchive "$@"

fi
