set input-meta on
set convert-meta off
set output-meta on
"\e[5~": beginning-of-history
"\e[6~": end-of-history
"\e[3~": delete-char
"\e[2~": quoted-insert
"\eOH": beginning-of-line
"\eOF": end-of-line
"\e[7~": beginning-of-line
"\e[8~": end-of-line
"\e[1~": beginning-of-line
"\e[4~": end-of-line
