# New wrapper for backwards compatibility
cd /etc/rc.d
[ -f pupBOOTfunctions -a -r pupBOOTfunctions ] && . pupBOOTfunctions
[ -f pupMOUNTfunctions -a -r pupMOUNTfunctions ] && . pupMOUNTfunctions
[ -f pupSOUNDfunctions -a -r pupSOUNDfunctions ] && . pupSOUNDfunctions
[ -f pupXfunctions -a -r pupXfunctions ] && . pupXfunctions
