#Use theme on all dialogs of Pmusic.
#Normally (false) only add individual gtk-theme to Main_window and source_window
#Tecnical: This don't reset gtktheme in $APPDIR/func
THEME_ALL_DIALOGS=false

#This overrides global gtk-theme.
#Only individual gtk-theme will show.
#Tecnical: This don't adds $GTK2_RC_FILES
ONLY_THIS_GTKTHEME=true
