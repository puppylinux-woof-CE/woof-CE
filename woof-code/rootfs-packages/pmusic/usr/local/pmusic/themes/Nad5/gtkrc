		state			= ACTIVE
		file			= "gtk_tab_top_out.png"
		border			= { 6,6,6,6 }
		stretch			= TRUE
		gap_side		= BOTTOM
		}

	image 
		{
		function		= EXTENSION
		recolorable		= TRUE
		file			= "gtk_tab_top_in.png"
		border			= { 6,6,6,6 }
		stretch			= TRUE
		gap_side		= BOTTOM
		}

	image 
		{
