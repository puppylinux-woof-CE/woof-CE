		recolorable		= TRUE
		overlay_file	= "gtk_checkbox_true.png"
		overlay_border	= { 0, 0, 0, 0 }
		overlay_stretch	= FALSE
		}
	image
		{
		function		= CHECK
		state 			= PRELIGHT
		shadow			= IN
		recolorable		= TRUE
		overlay_file	= "gtk_checkbox_true.png"
		overlay_border	= { 0, 0, 0, 0 }
		overlay_stretch	= FALSE
		}
	image 
		{
		function		= CHECK
		state			= INSENSITIVE
		shadow			= IN
		recolorable		= TRUE
		overlay_file	= "gtk_checkbox_insensitive.png"
		overlay_border	= { 0, 0, 0, 0 }
		overlay_stretch	= FALSE
		}
	}
}
