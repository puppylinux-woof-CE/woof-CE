style "radiobutton"	= "default"
{
engine "pixmap"
	{
	image
		{
		function		= OPTION
		shadow			= OUT
		recolorable		= TRUE
		overlay_file	= "gtk_checkbox_false.png"
		#overlay_border	= { 0, 0, 0, 0 }
		overlay_stretch	= FALSE
		}
	image
		{
		function		= OPTION
		shadow			= IN
		recolorable		= TRUE
		overlay_file	= "gtk_checkbox_true.png"
		#overlay_border	= { 0, 0, 0, 0 }
		overlay_stretch	= FALSE
		}
	image
		{
		function		= OPTION
		state			= INSENSITIVE
		recolorable		= TRUE
		overlay_file	= "gtk_checkbox_insensitive.png"
		#overlay_border	= { 0, 0, 0, 0 }
		overlay_stretch	= FALSE
		}
	}
}
